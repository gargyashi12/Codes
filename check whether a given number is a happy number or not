#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,num,sum,d,c=0;
    scanf("%d",&n);
    num=n;
    while(sum!=1 && sum!=4)
    {
        sum=0;
    while(n)
    {
        d=n%10;
        sum+=d*d;
        n=n/10;
    }
       n=sum;
    }
     if(sum==1)
     printf("%d is a Happy number\n",num);
     else
     printf("%d is not a Happy number\n",num);
}
